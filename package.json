{\n  "name": "midnightsociety-playwright",\n  "version": "1.0.0",\n  "description": "Playwright automation framework with Page Object Model for midnightsociety.com",\n  "main": "index.js",\n  "scripts": {\n    "test": "playwright test",\n    "test:debug": "playwright test --debug",\n    "test:headed": "playwright test --headed",\n    "test:ui": "playwright test --ui",\n    "test:chrome": "playwright test --project=chromium",\n    "test:firefox": "playwright test --project=firefox",\n    "test:safari": "playwright test --project=webkit",\n    "test:all": "playwright test",\n    "test:mobile": "playwright test responsiveDevices",\n    "test:ui-elements": "playwright test uiElements",\n    "test:social": "playwright test socialLogin",\n    "test:responsive": "playwright test responsiveDevices",\n    "test:single": "playwright test --workers=1",\n    "report": "playwright show-report",\n    "generate:docs": "node generate-test-documentation.js"\n  },\n  "keywords": ["playwright", "automation", "testing", "pom"],\n  "author": "",\n  "license": "ISC",\n  "devDependencies": {\n    "@playwright/test": "^1.48.0",\n    "dotenv": "^16.3.1",\n    "xlsx": "^0.18.5"\n  }\n}